{
    "dataRoles": [
        {
            "displayName": "Category",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Back Bar Value",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Front Bar Values",
            "name": "overlapValues",
            "kind": "Measure"
        },
        {
            "displayName": "Label Values",
            "name": "LabelValues",
            "kind": "Measure"
        },
        {
            "displayName": "Tooltip",
            "name": "tooltip",
            "kind": "Measure"
        }
    ],
    "drilldown": {
        "roles": [
            "category"
        ]
    },
    "sorting": {
        "default": {}
    },
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "tooltips"
        ]
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    },
                    "measure": {
                        "max": 1
                    },
                    "overlapValues": {
                        "max": 1
                    },
                    "LabelValues": {
                        "max": 1
                    },
                    "tooltip": {
                        "max": 4
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        },
                        {
                            "bind": {
                                "to": "LabelValues"
                            }
                        },
                        {
                            "bind": {
                                "to": "tooltip"
                            }
                        },
                        {
                            "bind": {
                                "to": "overlapValues"
                            }
                        }
                    ]
                }
            }
        }
    ],
